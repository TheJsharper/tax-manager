{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "rx-tax-manager": {
      "projectType": "application",
      "root": "projects/rx-tax-manager",
      "sourceRoot": "projects/rx-tax-manager/src",
      "prefix": "tax-manager",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/rx-tax-manager",
            "index": "projects/rx-tax-manager/src/index.html",
            "main": "projects/rx-tax-manager/src/main.ts",
            "polyfills": "projects/rx-tax-manager/src/polyfills.ts",
            "tsConfig": "projects/rx-tax-manager/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/rx-tax-manager/src/favicon.ico",
              "projects/rx-tax-manager/src/assets"
            ],
            "styles": [
              "projects/rx-tax-manager/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/rx-tax-manager/src/environments/environment.ts",
                  "with": "projects/rx-tax-manager/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "rx-tax-manager:build:production"
            },
            "development": {
              "browserTarget": "rx-tax-manager:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "rx-tax-manager:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "projects/rx-tax-manager/**/*.ts",
              "projects/rx-tax-manager/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/projects/rx-tax-manager"
          ],
          "options": {
            "jestConfig": "projects/rx-tax-manager/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "rx-tax-manager-e2e": {
      "root": "projects/rx-tax-manager-e2e",
      "sourceRoot": "projects/rx-tax-manager-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "projects/rx-tax-manager-e2e/cypress.json",
            "devServerTarget": "rx-tax-manager:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "rx-tax-manager:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "projects/rx-tax-manager-e2e/**/*.{js,ts}"
            ]
          }
        }
      },
      "tags": [],
      "implicitDependencies": [
        "rx-tax-manager"
      ]
    },
    "shared": {
      "projectType": "library",
      "root": "projects/shared",
      "sourceRoot": "projects/shared/src",
      "prefix": "tax-manager",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/projects/shared"
          ],
          "options": {
            "jestConfig": "projects/shared/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "projects/shared/**/*.ts",
              "projects/shared/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "tax-manager": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/tax-manager",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss",
              "src/theme.scss",
              "node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "tax-manager:build:production"
            },
            "development": {
              "browserTarget": "tax-manager:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tax-manager:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss",
              "node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "tax-manager-header": {
      "projectType": "library",
      "root": "projects/tax-manager-header",
      "sourceRoot": "projects/tax-manager-header/src",
      "prefix": "tax-manager",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/projects/tax-manager-header"
          ],
          "options": {
            "jestConfig": "projects/tax-manager-header/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "projects/tax-manager-header/**/*.ts",
              "projects/tax-manager-header/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "vat-calculator-backend": {
      "root": "projects/vat-calculator-backend",
      "sourceRoot": "projects/vat-calculator-backend/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nxrocks/nx-spring-boot:build",
          "options": {
            "root": "projects/vat-calculator-backend"
          }
        },
        "test": {
          "builder": "@nxrocks/nx-spring-boot:test",
          "options": {
            "root": "projects/vat-calculator-backend"
          }
        },
        "clean": {
          "builder": "@nxrocks/nx-spring-boot:clean",
          "options": {
            "root": "projects/vat-calculator-backend"
          }
        },
        "format": {
          "builder": "@nxrocks/nx-spring-boot:format",
          "options": {
            "root": "projects/vat-calculator-backend"
          }
        },
        "format-check": {
          "builder": "@nxrocks/nx-spring-boot:format-check",
          "options": {
            "root": "projects/vat-calculator-backend"
          }
        },
        "run": {
          "builder": "@nxrocks/nx-spring-boot:run",
          "options": {
            "root": "projects/vat-calculator-backend"
          }
        },
        "serve": {
          "builder": "@nxrocks/nx-spring-boot:serve",
          "options": {
            "root": "projects/vat-calculator-backend"
          }
        },
        "buildImage": {
          "builder": "@nxrocks/nx-spring-boot:buildImage",
          "options": {
            "root": "projects/vat-calculator-backend"
          }
        },
        "buildInfo": {
          "builder": "@nxrocks/nx-spring-boot:buildInfo",
          "options": {
            "root": "projects/vat-calculator-backend"
          }
        }
      },
      "tags": []
    }
  }
}
